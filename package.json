{
  "name": "@ltiseni94/rosreact",
  "version": "0.1.2",
  "description": "Ros client library for React, written in TypeScript",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run build:esm && npm run build:cjs && mkdir release && npm run pack",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "clean": "rm -rf dist && rm -rf release",
    "pack": "npm pack --pack-destination ./release/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Luca Tiseni",
  "license": "MIT",
  "keywords": [
    "ros",
    "reactjs",
    "react-native",
    "robotics"
  ],
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.17",
    "@types/roslib": "^1.1.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "js-sha512": "^0.8.0",
    "prop-types": "^15.8.1",
    "roslib": "^1.3.0"
  }
}
